{
  "name": "radar-dashboard",
  "version": "2.1.0",
  "license": "Apache 2.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:tools": "ng serve -c tools",
    "start:host": "ng serve --host 0.0.0.0",
    "build": "ng build --prod",
    "build:dev": "ng build",
    "all": "npm run fix:all && npm run lint && npm run test:coverage && npm run e2e",
    "lint": "npm run lint:ng && npm run lint:css && npm run lint:prettier && npm run lint:sort",
    "lint:ng": "ng lint",
    "lint:css": "stylelint 'src/**/*.scss'",
    "lint:prettier": "prettier --list-different '**/*.{js,ts,json}'",
    "lint:sort": "import-sort --list-different 'src/app/**/*.{js,ts}'",
    "test": "ng test",
    "test:sm": "ng test --source-map false",
    "test:coverage": "ng test --watch false --code-coverage && open coverage/index.html",
    "e2e": "ng e2e",
    "fix:all": "npm run fix:sort && npm run fix:css && npm run fix:prettier",
    "fix:css": "stylelint --fix 'src/**/*.scss'",
    "fix:prettier": "prettier --write '**/*.{js,ts,json}'",
    "fix:sort": "import-sort --write 'src/app/**/*.{js,ts}'"
  },
  "importSort": {
    ".js": {
      "parser": "babylon",
      "style": "module"
    },
    ".ts": {
      "parser": "typescript",
      "style": "module"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "not ie > 0"
  ],
  "dependencies": {
    "@angular/animations": "^6.0.7",
    "@angular/cdk": "^6.3.1",
    "@angular/common": "^6.0.7",
    "@angular/compiler": "^6.0.7",
    "@angular/core": "^6.0.7",
    "@angular/forms": "^6.0.7",
    "@angular/http": "^6.0.7",
    "@angular/material": "^6.3.1",
    "@angular/platform-browser": "^6.0.7",
    "@angular/platform-browser-dynamic": "^6.0.7",
    "@angular/router": "^6.0.7",
    "@auth0/angular-jwt": "^2.0.0",
    "@ngrx/effects": "^6.1.0",
    "@ngrx/entity": "^6.1.0",
    "@ngrx/router-store": "^6.1.0",
    "@ngrx/store": "^6.1.0",
    "@types/async": "^2.0.49",
    "@types/d3-shape": "^1.3.1",
    "core-js": "^2.6.5",
    "d3": "^5.5.0",
    "d3-line-chunked": "^1.4.1",
    "d3-selection-multi": "^1.0.1",
    "hammerjs": "^2.0.8",
    "rxjs": "^6.3.0",
    "shortid": "^2.2.14",
    "ts-helpers": "^1.1.2",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.8",
    "@angular/cli": "^7.3.8",
    "@angular/compiler-cli": "^7.2.14",
    "@angular/language-service": "^6.0.7",
    "@ngrx/schematics": "^12.2.0",
    "@ngrx/store-devtools": "^6.1.0",
    "@types/d3": "^5.7.2",
    "@types/hammerjs": "^2.0.35",
    "@types/jasmine": "^2.8.3",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^11.13.8",
    "browserstack-local": "^1.3.0",
    "codacy-coverage": "^3.4.0",
    "codelyzer": "^4.4.1",
    "coveralls": "^3.0.0",
    "import-sort-cli": "^5.0.0",
    "import-sort-parser-babylon": "^5.0.0",
    "import-sort-parser-typescript": "^5.0.0",
    "import-sort-style-module": "^5.0.0",
    "jasmine": "^2.99.0",
    "jasmine-core": "^2.99.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.1.0",
    "karma-browserstack-launcher": "^1.5.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-scss-preprocessor": "^3.0.0",
    "karma-spec-reporter": "^0.0.32",
    "ngrx-store-freeze": "^0.2.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.17.0",
    "protractor": "~5.4.2",
    "rxjs-tslint": "0.1.5",
    "sass-mq": "^5.0.0",
    "stylelint": "^11.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-order": "^3.1.1",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "tslint-angular": "^1.1.2",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "~3.1.6"
  }
}
